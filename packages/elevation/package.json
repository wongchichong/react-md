    "csstype": "^3.1.2",
    "jest": "^29.5.0",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "sass": "^1.62.1",
    "typescript": "^5.0.4",
    "vite": "^4.3.3",
