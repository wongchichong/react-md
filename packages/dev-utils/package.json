{
  "name": "@react-md/dev-utils",
  "version": "5.1.3",
  "description": "Common build utilities for react-md. Contains helpers for building, watching, testing, etc",
  "private": true,
  "bin": {
    "dev-utils": "./bin/cli.js"
  },
  "scripts": {
    "release": "git add . && git commit -m \"Bump version\" && git push && npm version patch",
    "clean": "rimraf bin dist",
    "executable": "node executable.js",
    "compile": "tsc",
    "build": "npm-run-all compile executable",
    "watcher": "chokidar \"bin/cli.js\" -c \"npm run executable\"",
    "watch": "npm-run-all -p \"compile -w\" watcher"
  },
  "dependencies": {
    "@babel/core": "^7.21.5",
    "@octokit/core": "^3.6.0",
    "chokidar": "^3.5.3",
    "commander": "^9.5.0",
    "cssnano": "^5.1.15",
    "dotenv": "^16.0.3",
    "filesize": "^8.0.7",
    "fs-extra": "^10.1.0",
    "glob": "^8.1.0",
    "gzip-size": "^6.0.0",
    "he": "^1.2.0",
    "lodash": "^4.17.21",
    "loglevel": "^1.8.1",
    "markdown-toc": "^1.2.0",
    "marked": "^4.3.0",
    "npm-run-all": "^4.1.5",
    "pnpm": "^8.3.1",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^7.8.3",
    "pretty-ms": "^7.0.1",
    "prompts": "^2.4.2",
    "react2voby": "workspace:../../../react2voby",
    "rimraf": "^3.0.2",
    "sassdoc": "^2.7.4",
    "ts-morph": "^14.0.0",
    "typedoc": "^0.22.18"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.0",
    "@types/he": "^1.2.0",
    "@types/jest": "^29.5.1",
    "@types/marked": "^4.0.8",
    "@types/prompts": "^2.4.4",
    "@types/sass": "^1.45.0",
    "chokidar-cli": "^3.0.0",
    "csstype": "^3.1.2",
    "jest": "^29.5.0",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "sass": "^1.62.1",
    "typescript": "^5.0.4",
    "vite": "^4.3.3",
    "vite-plugin-sass-dts": "^1.3.4",
    "vite-plugin-static-copy": "^0.13.1"
  },
  "peerDependencies": {
    "voby": "workspace:../../../woby"
  }
}
